class Solution {
    public int[] twoSum(int[] nums, int t) {
        Map<Integer,Integer> mp=new HashMap<>();
        int a[]=new int[2];
     //   for(int i=0;i<nums.length;i++) mp.put(nums[i],i);
        
       for(int i=0;i<nums.length;i++)
       {
           if(mp.containsKey(t-nums[i]))
           {
               a[0]=i;
               a[1]=mp.get(t-nums[i]);
               return a;
           }
           mp.put(nums[i],i);
       }
        return a;
    }
}
